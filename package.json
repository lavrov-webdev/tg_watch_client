		"@types/node": "^22.13.13",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"esno": "^4.8.0",
		"express": "^4.21.2",
